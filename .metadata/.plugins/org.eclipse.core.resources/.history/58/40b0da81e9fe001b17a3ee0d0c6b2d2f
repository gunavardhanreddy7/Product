package com.example.demo;

import java.util.ArrayList;
import java.util.List;

public class ProductList {
	
	
	    private List<Product> pd;

	    public ProductList() {
	        pd = new ArrayList<>();
	    }
	    
	    public getProducts() {
	    	return pd;
	    }

	    // standard constructor and getter/setter
	}


